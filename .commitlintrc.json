{
  "extends": ["@commitlint/config-conventional"],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "init",
        "docs",
        "dev",
        "feature",
        "fix",
        "hotfix",
        "improvement",
        "refactor",
        "test",
        "chore"
      ]
    ]
  }
}
